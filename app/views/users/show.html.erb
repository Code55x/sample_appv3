<% provide(:title, @user.name) %>
  <div class="row">
    <aside class="col-md-4">
      <section class="user_info">
        <h1>
          <%= gravatar_for @user %>
          <%= @user.name %>
        </h1>
      </section>
    </aside>
  </div>
  <div>
    <ul>
      <li>Name:
        <%= @user.name%></li>
      <li>Email:
        <%= @user.email%></li>
      <li>Time create:
        <%= @user.created_at%></li>
      <li>Time Update:
        <%= @user.updated_at%></li>
    </ul>
  </div>
  <div class="">
    Time:
    <%= Time.now%>
  </div>
